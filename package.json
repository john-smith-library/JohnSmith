{
  "name": "john-smith",
  "version": "4.0.1-alpha.19",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts --ext .tsx",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prebuild": "node -p \"'export const JS_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/version.ts",
    "build": "tsc",
    "build_demo": "cd src_demo && webpack",
    "build_demo_local": "cd src_demo_local && webpack",
    "run_demo_local": "cd src_demo_local && webpack --watch",
    "build_docs": "typedoc --theme default --exclude \"**/*+(spec|index).ts\" --includes src_docs --out dist_doc --entryPointStrategy expand --excludeReferences --plugin typedoc-plugin-mermaid --categorizeByGroup true src",
    "pack": "npm run prebuild && npm run clean && npm run build && cpy package.json dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guryanovev/JohnSmith.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guryanovev/JohnSmith/issues"
  },
  "homepage": "https://github.com/guryanovev/JohnSmith#readme",
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "cpy-cli": "5.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "glob": "10.3.12",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "typedoc": "0.25.13",
    "typedoc-plugin-merge-modules": "5.1.0",
    "typedoc-plugin-mermaid": "1.10.0",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4"
  }
}
